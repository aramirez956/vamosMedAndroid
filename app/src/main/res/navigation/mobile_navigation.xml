<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.medellin.vamosmed.ui.viaje.ViajeFragment"
        android:label="@string/title_bici"
        tools:layout="@layout/fragment_viaje" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.medellin.vamosmed.ui.filtros.FiltrosFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_filtros" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.medellin.vamosmed.ui.estadisticas.EstadisticasFragment"
        android:label="@string/title_estadistica"
        tools:layout="@layout/fragment_estadisticas" />
</navigation>